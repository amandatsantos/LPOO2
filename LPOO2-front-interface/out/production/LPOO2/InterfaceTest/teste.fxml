<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <BarChart fx:id="humidityChart" prefHeight="400.0" prefWidth="600.0" title="Umidade Relativa do Ar">
        <xAxis>
            <CategoryAxis label="Dias da Semana"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Umidade (%)" />
        </yAxis>
    </BarChart>
    <BarChart fx:id="temperatureChart" prefHeight="400.0" prefWidth="600.0" title="Medição da Temperatura">
        <xAxis>
            <CategoryAxis label="Dias da Semana"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Temperatura (°C)"/>
        </yAxis>
    </BarChart>
</VBox>
